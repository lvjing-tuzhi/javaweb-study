<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
    
    <context-param>
        <param-name>name</param-name>
        <param-value>吕静</param-value>
    </context-param>


    <servlet>
        <servlet-name>setc</servlet-name>
        <servlet-class>com.tuzhi.servlet.SetContext</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setc</servlet-name>
        <url-pattern>/setc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getc</servlet-name>
        <servlet-class>com.tuzhi.servlet.GetContext</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getc</servlet-name>
        <url-pattern>/getc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>error</servlet-name>
        <servlet-class>com.tuzhi.servlet.Error</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>error</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>initParame</servlet-name>
        <servlet-class>com.tuzhi.servlet.InitParame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>initParame</servlet-name>
        <url-pattern>/initParame</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>properties</servlet-name>
        <servlet-class>com.tuzhi.servlet.PropertiesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>properties</servlet-name>
        <url-pattern>/properties</url-pattern>
    </servlet-mapping>
</web-app>
